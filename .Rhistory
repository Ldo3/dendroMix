#### 1d example
library(mixtools)
set.seed(123)
# sample sizes
n <- 3000
# true mixture parameters
pi_true <- c(0.3, 0.4, 0.3)      # mixing proportions
mu_true <- c(-2, 0, 3)           # means
sd_true <- c(0.5, 1, 0.7)        # standard deviations
# generate data
z <- sample(1:3, size = n, replace = TRUE, prob = pi_true)
X <- rnorm(n, mean = mu_true[z], sd = sd_true[z])
# fit mixture of 3 normals
fit <- normalmixEM(X, k = 3, maxit = 1000)
summary(fit)
mix_density <- function(x, fit) {
sapply(x, function(xx) {
sum(fit$lambda * dnorm(xx, mean = fit$mu, sd = fit$sigma))
})
}
# plot histogram + fitted densities
hist(X, breaks = 50, freq = FALSE, col = "lightgray",
main = "1D Mixture of 3 Gaussians fitted with mixtools")
curve(mix_density(x, fit), from = min(X), to = max(X),
add = TRUE, col = "black", lwd = 2)
# add each component density
for (j in 1:3) {
curve(fit$lambda[j] * dnorm(x, fit$mu[j], fit$sigma[j]),
add = TRUE, col = j, lwd = 2, lty = 2)
}
## overfit and merge down
# fit mixture of 3 normals
K = 5
fit <- normalmixEM(X, k = K, maxit = 1000)
summary(fit)
mix_density <- function(x, fit) {
sapply(x, function(xx) {
sum(fit$lambda * dnorm(xx, mean = fit$mu, sd = fit$sigma))
})
}
# plot histogram + fitted densities
hist(X, breaks = 50, freq = FALSE, col = "lightgray",
main = "1D Mixture of 3 Gaussians fitted with mixtools")
curve(mix_density(x, fit), from = min(X), to = max(X),
add = TRUE, col = "black", lwd = 2)
# add each component density
for (j in 1:K) {
curve(fit$lambda[j] * dnorm(x, fit$mu[j], fit$sigma[j]),
add = TRUE, col = j, lwd = 2, lty = 2)
}
dmm = dendrogram_mixing(ps = fit$lambda, thetas = fit$mu)
plot(dmm$hc, hang=-1)
dmm$Gs[[2]]$thetas
plot_dendrogram_mixing(dmm, atom_size = 1.5, lwd=1.5)
## weak identifiable
dmm = dendrogram_mixing(ps = fit$lambda, thetas = fit$mu, sigmas = fit$sigma)
plot(dmm$hc, hang=-1)
dmm$Gs[[3]]$thetas
plot_dendrogram_mixing(dmm, atom_size = 1.5, lwd=1.5)
dmm$Gs[[3]]$sigmas
